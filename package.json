{
  "name": "anylogger-loglevel",
  "version": "1.1.0-beta.1",
  "description": "Anylogger adapter for loglevel",
  "type": "module",
  "exports": {
    ".": {
      "import": "./anylogger-loglevel.js",
      "require": "./anylogger-loglevel.cjs"
    }
  },
  "src": "anylogger-loglevel.ts",
  "main": "anylogger-loglevel.js",
  "cjs": "anylogger-loglevel.cjs",
  "iife": "anylogger-loglevel.iife.js",
  "min": "anylogger-loglevel.min.js",
  "spec": "anylogger-loglevel.spec.js",
  "files": [
    "anylogger-loglevel.cjs",
    "anylogger-loglevel.d.ts",
    "anylogger-loglevel.d.ts.map",
    "anylogger-loglevel.iife.js",
    "anylogger-loglevel.js",
    "anylogger-loglevel.js.map",
    "anylogger-loglevel.min.js",
    "anylogger-loglevel.spec.js",
    "anylogger-loglevel.ts",
    "test.html",
    "tsconfig.json"
  ],
  "scripts": {
    "build": "tsc && npm run package && npm run minify && npm run docs",
    "clean": "rimraf anylogger-loglevel.cjs anylogger-loglevel.d.ts anylogger-loglevel.d.ts.map anylogger-loglevel.iife.js anylogger-loglevel.js anylogger-loglevel.js.map anylogger-loglevel.min.js",
    "docs": "cross-env NODE_ENV=production node build.js docs",
    "minify": "cross-env NODE_ENV=production node build.js minify",
    "package": "cross-env NODE_ENV=production rollup -c",
    "prepare": "npm run test",
    "test": "npm run build && mocha anylogger-loglevel.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/download/anylogger-loglevel.git"
  },
  "keywords": [
    "anylogger",
    "loglevel",
    "anylogger-loglevel",
    "log"
  ],
  "author": "Stijn de Witt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/download/anylogger-loglevel/issues"
  },
  "homepage": "https://github.com/download/anylogger-loglevel#readme",
  "peerDependencies": {
    "anylogger": "1.x || >=1.1.0-beta || >=1.2.0-beta || >=1.3.0-beta || >=1.4.0-beta || >=1.5.0-beta || >=1.6.0-beta || >=1.7.0-beta || >=1.8.0-beta || >=1.9.0-beta",
    "loglevel": "1.x"
  },
  "devDependencies": {
    "anylogger": "^1.1.0-beta.4",
    "chai": "^5.1.0",
    "cross-env": "^7.0.3",
    "gzip-size": "^7.0.0",
    "loglevel": "^1.9.1",
    "mocha": "^10.3.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.12.0",
    "sinon": "^17.0.1",
    "typescript": "^5.3.3",
    "uglify-js": "^3.17.4"
  }
}
